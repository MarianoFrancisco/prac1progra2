<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LectorArchivo</servlet-name>
        <servlet-class>Servlet.LectorArchivo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InformacionPieza</servlet-name>
        <servlet-class>CondicionalesFabrica.InformacionPieza</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcesoEnsamble</servlet-name>
        <servlet-class>CondicionalesFabrica.ProcesoEnsamble</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertarMueble</servlet-name>
        <servlet-class>CondicionalesFabrica.InsertarMueble</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LectorArchivo</servlet-name>
        <url-pattern>/LectorArchivo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InformacionPieza</servlet-name>
        <url-pattern>/InformacionPieza</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcesoEnsamble</servlet-name>
        <url-pattern>/ProcesoEnsamble</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertarMueble</servlet-name>
        <url-pattern>/InsertarMueble</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
